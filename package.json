{
  "name": "blog.stephenlam.me",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "gulp",
    "minify:html": "html-minifier --output-dir _site --input-dir _site --file-ext html --collapse-whitespace --remove-comments --sort-class-name index.html",
    "build:gulp": "gulp build",
    "build:jekyll": "cross-env JEKYLL_ENV=production bundle exec jekyll build",
    "build:all": "yarn run build:gulp && yarn run build:jekyll && yarn run minify:html"
  },
  "repository": "git@github.com:ascendancyy/stephenlam.me.git",
  "author": "Stephen Lam <ascendancy.sl@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "bourbon": "^4.3.4",
    "browser-sync": "^2.18.13",
    "cross-env": "^5.0.5",
    "cssnano": "^3.10.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-postcss": "^7.0.0",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-uglify": "^3.0.0",
    "gulp-util": "^3.0.8",
    "html-minifier": "^3.5.3",
    "node-bourbon": "^4.2.8",
    "postcss-flexbugs-fixes": "^3.2.0",
    "sass-lint": "^1.10.2",
    "uglify-js": "^3.0.28"
  }
}
